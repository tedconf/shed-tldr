{
  "name": "shed-tldr",
  "version": "0.1.0",
  "description": "quick lookup for shed classes",
  "main": "index.js",
  "repository": "http://github.com/tedconf/shed-tldr",
  "author": "RÃ´mulo Machado",
  "license": "MIT",
  "scripts": {
    "generate": "plop --plopfile config/plopfile.js",
    "gen": "yarn generate",
    "g": "yarn generate",
    "start": "run-p 'dev' 'storybook'",
    "dev": "env NODE_ENV=development webpack-dev-server",
    "build-app": "del dist && env NODE_ENV=production webpack --colors --display-provided-exports --display-used-exports",
    "build": "run-p 'build-app' 'build-storybook'",
    "pretest": "yarn lint; yarn lint:typecheck",
    "lint": "eslint src",
    "lint:fix": "yarn lint -- --fix",
    "deploy": "echo 'not conrigured, please edit this task in your package.json'",
    "lint:typecheck": "flow; test $? -eq 0 -o $? -eq 2",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "storybook": "start-storybook -p 2001",
    "build-storybook": "mkdir -p dist && build-storybook -o dist/storybook"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "setupFiles": [
      "<rootDir>/config/test-setup.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/hot.js"
    ],
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/config/fileMock.js"
    },
    "roots": [
      "<rootDir>/src"
    ]
  },
  "babel": {
    "presets": [
      "react",
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "stage-0"
    ],
    "plugins": [
      "shed",
      "syntax-dynamic-import",
      "syntax-flow",
      "transform-flow-strip-types",
      "transform-runtime"
    ],
    "env": {
      "development": {
        "plugins": [
          "shed",
          "react-hot-loader/babel",
          "syntax-dynamic-import",
          "syntax-flow",
          "transform-flow-strip-types",
          "transform-runtime"
        ]
      },
      "production": {
        "plugins": [
          "shed",
          [
            "lodash",
            {
              "id": [
                "lodash",
                "recompose"
              ]
            }
          ],
          "transform-react-router-optimize"
        ],
        "presets": [
          "react-optimize"
        ]
      },
      "test": {
        "presets": [
          "react",
          [
            "es2015",
            {
              "modules": false
            }
          ],
          "stage-0"
        ],
        "plugins": [
          "shed",
          "react-hot-loader/babel",
          "syntax-dynamic-import",
          "syntax-flow",
          "transform-flow-strip-types",
          "transform-flow-strip-types",
          "transform-runtime",
          "transform-es2015-modules-commonjs"
        ]
      }
    }
  },
  "eslintConfig": {
    "plugins": [
      "flowtype"
    ],
    "env": {
      "jest": true,
      "browser": true
    },
    "globals": {
      "it": true,
      "describe": true,
      "test": true,
      "expect": true,
      "react": true,
      "mount": true,
      "shallow": true,
      "render": true,
      "React": true
    },
    "parser": "babel-eslint",
    "extends": [
      "airbnb",
      "plugin:flowtype/recommended"
    ],
    "rules": {
      "react/no-unused-prop-types": 0,
      "react/no-danger": 0,
      "react/require-default-props": 0,
      "no-debugger": 1,
      "no-mixed-operators": 0,
      "no-nested-ternary": 0,
      "no-confusing-arrow": 0,
      "react/jsx-filename-extension": 0,
      "import/extensions": 0,
      "import/no-named-as-default-member": 0,
      "import/no-named-as-default": 0,
      "import/no-unresolved": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-absolute-path": 0,
      "import/no-duplicates": 0,
      "react/jsx-no-undef": 0,
      "no-unused-vars": [
        "error",
        {
          "varsIgnorePattern": "Shed"
        }
      ]
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "webpack.config.js"
        }
      }
    }
  },
  "devDependencies": {
    "@storybook/addon-centered": "^3.0.0",
    "@storybook/addon-knobs": "^3.0.0",
    "@storybook/addon-options": "^3.0.0",
    "@storybook/addons": "^3.0.0",
    "@storybook/react": "^3.0.0",
    "babel-core": "^6.20.0",
    "babel-eslint": "^7.2.1",
    "babel-jest": "^20.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-flow-react-proptypes": "^3.1.2",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-shed": "^1.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.4",
    "babel-plugin-transform-react-router-optimize": "^1.0.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.16.0",
    "color-string": "1.5.x",
    "css-loader": "0.14.5",
    "del": "^2.2.2",
    "del-cli": "^1.0.0",
    "enzyme": "^2.8.2",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-import-resolver-webpack": "^0.8.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.0",
    "file-loader": "^0.11.1",
    "flow-bin": "^0.46.0",
    "global": "^4.3.1",
    "html-webpack-plugin": "^2.28.0",
    "isparta": "^4.0.0",
    "jest": "^20.0.0",
    "lodash-webpack-plugin": "^0.11.3",
    "npm-run-all": "^4.0.2",
    "passwd-user": "^2.1.0",
    "plop": "^1.8.0",
    "postcss-cssnext": "^2.11.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.5",
    "preload-webpack-plugin": "^1.2.2",
    "prop-types": "^15.5.10",
    "react-addons-test-utils": "^15.5.1",
    "react-dev-utils": "^1.0.1",
    "stats-webpack-plugin": "^0.6.0",
    "style-loader": "^0.17.0",
    "webpack": "2.x.x",
    "webpack-dev-server": "2.4.x",
    "webpack-manifest-plugin": "1.1.x",
    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "4.x.x"
  },
  "dependencies": {
    "babel-runtime": "6.23.x",
    "classnames": "2.2.x",
    "glamor": "2.20.x",
    "glamorous": "3.19.x",
    "modularscale": "^2.0.1",
    "offline-plugin": "4.x.x",
    "react": "15.x.x",
    "react-dom": "15.x.x",
    "react-hot-loader": "next",
    "react-loadable": "3.x.x",
    "react-media": "vinspee/react-media#d6d66c97378067d5c0907bdb2f9d81b90ff7c9a4",
    "react-router": "4.x.x",
    "react-router-dom": "4.x.x",
    "react-shed": "3.0.0-alpha.6",
    "recompose": "0.23.x"
  }
}
